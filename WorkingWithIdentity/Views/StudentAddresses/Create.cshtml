@model WorkingWithIdentity.ViewModels.StudentAddressViewModel

@{
    ViewData["Title"] = "Create";
}
<!DOCTYPE html>
<html>
<body>
    <h2>Create</h2>

    <h4>StudentAddress</h4>
    <hr />
    <div class="row">
        <div class="col-md-4">
            <form asp-action="Create">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <label asp-for="Address" class="control-label"></label>
                    <input asp-for="Address" class="form-control" />
                    <span asp-validation-for="Address" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="StudentName" class="control-label"></label>
                    <select asp-for="StudentName" class="form-control" asp-items="ViewBag.StudentName"></select>
                </div>
                <div class="form-group">
                    <input type="submit" value="Create" class="btn btn-default" />
                </div>
            </form>
        </div>
    </div>
    <ul id="myList">
        <li>Coffee</li>
        <li>Tea</li>
    </ul>
    <div>
        <a asp-action="Index">Back to List</a>
    </div>

</body>
</html>